{
  "buildCommand": "cd ../.. && pnpm run build:csr",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "devCommand": "cd ../.. && pnpm --filter feed-csr-next run dev",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_MOCK_API_DOMAIN": "ssr-mock-api.vercel.app"
  }
}
